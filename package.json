{
  "name": "ai_commander",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "module": "dist/es/main.js",
  "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
  "scripts": {
    "clean": "rimraf -rf tsc-out && rimraf -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run clean",
    "build": "SET NODE_ENV=development && rollup -c",
    "push-local": "rollup -c --environment DEST:localserver",
    "push:of-sim": "SET NODE_ENV=development && npm run clean && rollup -c --environment DEST:official-sim",
    "push:of-main": "SET NODE_ENV=production && npm run clean && rollup -c --environment DEST:official-main",
    "push:ap-sim": "SET NODE_ENV=development && npm run clean && rollup -c --environment DEST:animapix-sim",
    "push:ap-main": "SET NODE_ENV=production && npm run clean && rollup -c --environment DEST:animapix-main",
    "push-season": "rollup -c --environment DEST:season",
    "push-sim": "rollup -c --environment DEST:sim",
    "push:sf-sim": "SET NODE_ENV=production && npm run clean && rollup -c --environment DEST:iklem-sim",
    "push:sf-main": "SET NODE_ENV=production && rollup -c --environment DEST:iklem-main",
    "test": "npm run test-unit",
    "test-unit": "mocha test/unit/**/*.ts",
    "test-integration": "echo 'See docs/in-depth/testing.md for instructions on enabling integration tests'",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-season": "rollup -cw --environment DEST:season",
    "watch-sim": "rollup -cw --environment DEST:sim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screepers/screeps-typescript-starter.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/screepers/screeps-typescript-starter/issues"
  },
  "homepage": "https://github.com/screepers/screeps-typescript-starter#readme",
  "engines": {
    "node": "10.x || 12.x"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "source-map": "~0.6.1"
  },
  "devDependencies": {
    "@lopatnov/rollup-plugin-uglify": "^2.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/chai": "^4.1.6",
    "@types/lodash": "^4.14.199",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.1",
    "@types/screeps": "^3.3.4",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@typescript-eslint/typescript-estree": "^4.29.1",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "minify": "^10.5.2",
    "mocha": "^5.2.0",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}